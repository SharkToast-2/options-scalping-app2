{
  "version": "1.0",
  "log_level": "INFO",
  "security": {
    "mask_sensitive_data": true,
    "encrypt_logs": false,
    "log_rotation": true,
    "max_log_size": "10MB",
    "retention_days": 30
  },
  "formats": {
    "security_events": {
      "format": "%(asctime)s - %(levelname)s - %(message)s",
      "include_ip": false,
      "mask_keys": true
    }
  }
}